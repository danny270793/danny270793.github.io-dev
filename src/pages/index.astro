---
import "../../public/css/w3css.css";
import "../../public/css/index.css";
import "../../public/fonts/fontawesome/css/index.css";
import { Image } from "astro:assets";
import SocialLinks, { SocialLink } from "../libraries/me/social-links";
import Educations, { Education } from "../libraries/me/knowledges/educations";
import Me from "../libraries/me";
import Certifications, {
  Certification,
} from "../libraries/me/knowledges/certifications";
import Libraries, { Library } from "../libraries/me/knowledges/libraries";
import Projects, { Project } from "../libraries/me/knowledges/projects";

import profilePicture from "../../public/images/profile.jpg";
import headerBackground from "../../public/images/parallax/header.jpg";
import educationBackground from "../../public/images/parallax/education.jpg";
import certificationsBackground from "../../public/images/parallax/certification.jpg";
import openSourceLibrariesBackground from "../../public/images/parallax/open source libraries.jpg";
import projectsBackground from "../../public/images/parallax/projects.jpg";
import GooglePlay from "../../public/images/projects/google-play.png";
import AppStore from "../../public/images/projects/app-store.png";

Array.prototype.chunk = function(chunkSize) {
    let result = [];

    for (let i = 0; i < this.length; i += chunkSize) {
        let chunk = this.slice(i, i + chunkSize);
        result.push(chunk);
    }

    return result;
}
---

<html lang="es">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <title>Danny Vaca</title>
  </head>
  <di>
    <div
      class="parallax header w3-display-container"
      style={`background-image: url("${headerBackground.src}")`}
    >
      <div class="w3-display-middle no-spaces">
        <span
          class="w3-center w3-padding w3-black w3-xlarge w3-wide w3-animate-opacity"
        >
          Danny Vaca
        </span>
      </div>
    </div>

    <div class="w3-content w3-container w3-padding-64">
      <h3 class="w3-center">About me</h3>
      <div class="w3-row">
        <div class="w3-quarter w3-center">
          <Image
            class="w3-circle w3-padding"
            src={profilePicture}
            alt="Profile picture"
            style="width: auto; height: 200px;"
          />
        </div>
        <div class="w3-threequarter w3-padding">
          {Me.detail.split("\n").map((e) => <div>{e}</div>)}
        </div>
      </div>
    </div>

    <div
      class="parallax separator w3-display-container"
      style={`background-image: url("${educationBackground.src}")`}
    >
      <div class="w3-display-middle no-spaces">
        <span
          class="w3-center w3-padding w3-black w3-xlarge w3-wide w3-animate-opacity"
        >
          Education
        </span>
      </div>
    </div>

    <div class="w3-content w3-container w3-padding-64">
      {
        [Educations[0]].map((education: Education) => (
          <div class="w3-row padded-content">
            <div class="w3-third w3-center">
              <Image
                class="w3-padding"
                src={education.image}
                alt={education.name}
                style="width: 100%; height: auto;"
              />
            </div>
            <h class="w3-twothird w3-padding w3-center">
              <h5>
                <strong>{education.name}</strong>
              </h5>
              <p>{education.institution}</p>
              <h6 class="w3-text-teal">
                <i class="far fa-calendar-alt w3-margin-right">
                  {education.from} - {education.to}
                </i>
              </h6>
            </h>
          </div>
        ))
      }
      {
        Educations.slice(1).chunk(2).map((educations: Education[]) => (
        <div class="w3-row">
          {educations.map((education: Education) => (
          <div class="w3-half">
            <div class="w3-third w3-center">
              <Image
                class="w3-padding"
                src={education.image}
                alt={education.name}
                style="width: 100%; height: auto;"
              />
            </div>
            <h class="w3-twothird w3-padding w3-center">
              <h5>
                <strong>{education.name}</strong>
              </h5>
              <p>{education.institution}</p>
              <h6 class="w3-text-teal">
                <i class="far fa-calendar-alt w3-margin-right">
                  {education.from} - {education.to}
                </i>
              </h6>
            </h>
          </div>
          ))}
        </div>
        ))
      }
    </div>

    <div
      class="parallax separator w3-display-container"
      style={`background-image: url("${certificationsBackground.src}")`}
    >
      <div class="w3-display-middle no-spaces">
        <span
          class="w3-center w3-padding w3-black w3-xlarge w3-wide w3-animate-opacity"
        >
          Certifications
        </span>
      </div>
    </div>
    <div class="w3-content w3-container w3-padding-64">
      {
        Certifications.sort((a, b) => b.date - a.date).chunk(3).map((certifications: Certification[]) => (
        <div class="w3-row">
          {certifications.map((certification: Certification) => (
          <div class="w3-third w3-center w3-padding">
            <div class="w3-hover-shadow w3-padding">
              <a href={certification.link} target="_blank">
                <Image
                  class="w3-center"
                  src={certification.image}
                  alt={certification.name}
                  style="width: 100%; height: auto;"
                />
                <h5>
                  <strong>
                    {certification.code === ""
                      ? certification.name
                      : `(${certification.code}) - ${certification.name}`}
                  </strong>
                </h5>
                <p>{certification.brand}</p>
                <h6 class="w3-text-teal">
                  <i class="far fa-calendar-alt w3-margin-right">
                    {certification.date.toISOString().slice(0, 10)}
                  </i>
                </h6>
              </a>
            </div>
          </div>
          ))}
        </div>
        ))
      }
    </div>

    <div
      class="parallax separator w3-display-container"
      style={`background-image: url("${openSourceLibrariesBackground.src}")`}
    >
      <div class="w3-display-middle no-spaces">
        <span
          class="w3-center w3-padding w3-black w3-xlarge w3-wide w3-animate-opacity"
        >
          Open Source Libraries
        </span>
      </div>
    </div>
    <div class="w3-content w3-container w3-padding-64">
      {
        Libraries.chunk(2).map((libraries: Library[]) => (
        <div class="w3-row">
          {
            libraries.map((library: Library) => (
            <div class="w3-half w3-center w3-padding">
              
              <div class="w3-row w3-hover-shadow">
                <div class="w3-third w3-center">
                  <Image
                    class="w3-padding"
                    src={library.image}
                    alt={library.name}
                    style="width: auto; height: 100px;"
                  />
                </div>
                <div class="w3-twothird w3-padding w3-center">
                  <h5>
                    <strong>{library.name}</strong>
                  </h5>
                  <p>{library.description}</p>
                </div>
              </div>
              </div>
            ))
          }
          </div>
        ))
      }
    </div>

    <div
      class="parallax separator w3-display-container"
      style={`background-image: url("${projectsBackground.src}")`}
    >
      <div class="w3-display-middle no-spaces">
        <span
          class="w3-center w3-padding w3-black w3-xlarge w3-wide w3-animate-opacity"
        >
          Projects
        </span>
      </div>
    </div>
    <div class="w3-content w3-container w3-padding-64">
      {
        Projects.map((project: Project) => (
          <div class="w3-row padded-content">
            <div class="w3-padding w3-center">
              <h5>
                <strong>{project.name}</strong>
              </h5>
              <p>{project.description}</p>
              {project.playstore !== undefined ? (
                <Image
                  class="w3-padding"
                  src={GooglePlay}
                  alt={project.name}
                  style="width: auto; height: 50px;"
                />
              ) : (
                <span />
              )}
            </div>
          </div>
        ))
      }
    </div>

